@{
    var ListOtherPhone = new List<dynamic>();
    //ListOtherPhone.Add(new { HyperLinkTell = "tel:(840)2201009", TextTell = "(840)-220-1009" });
    var Button_Contact_Rounded = ViewBag.Button_Contact_Rounded ?? "0";
}
<section class="py-4 container-lg">
    <div class="row">
        <div class="col-lg-6">
            <div class="box-tel">
                <div class="outline-button-theme w-100">
                    <a href="@ViewBag.HyperLinkTell" target="_self" class="btn btn-default btn-theme btn-none-border btn-border-black btn-tel rounded rounded-@(Button_Contact_Rounded)">
                        <span>@ViewBag.TextTell</span>
                    </a>
                </div>
            </div>
        </div>
        <div class="col-lg-6">
            <div class="box-contact">
                <div class="outline-button-theme w-100">
                    <a href="@ViewBag.LinkGoogleMapAddress" target="_blank" class="btn btn-default btn-theme btn-none-border btn-border-black btn-contact rounded rounded-@(Button_Contact_Rounded)">
                        <span>@ViewBag.Address</span>
                    </a>
                </div>
            </div>
        </div>
    </div>
    <div class="row">
        <div class="col-lg-12">
            <div class="box-map">
                <iframe src="@ViewBag.LinkIFrameGoogleMap" width="100%" height="450" style="border:0;" allowfullscreen="" loading="lazy" referrerpolicy="no-referrer-when-downgrade"></iframe>
            </div>
        </div>
    </div>
</section>