    <script type="text/javascript">
        ActiveMenu("menu-e-gift");

        var sliderInit = $('.sliders');

        $(document).ready(function () {
            sliderInit.slick({
                dots: false,
                infinite: true,
                slidesToShow: 1,
                arrows: false,
                autoplay: true,
                autoplaySpeed: 4000,
                fade: true
            });
            sliderInit.slickAnimation();
        });
