@{
    ViewBag.Title = "Price List";
    var Prices = (List<WebNails.Models.PricesModel>)ViewBag.Prices;
    Prices = Prices.Where(x => !string.IsNullOrEmpty(x.Src) && x.Status).ToList();
}
@section css{
    <link href="~/Content/css/bootstrap-pdf-viewer.css" rel="stylesheet" />
    <link href="~/Content/css/page-prices.css" rel="stylesheet">
}
<main class="bg-main">
    <div class="overlay"></div>
    <div class="page-main">
        <section class="py-2 container">
            <div class="row justify-content-center">
                <div class="col-lg-12 py-2">
                    <div class="price-list-pdf" style="width: 100%; height:800px;">
                        <div id="viewer" class="pdf-viewer" data-url="/Content/images/price-list/price-list.pdf"></div>
                    </div>
                </div>
            </div>
        </section>
    </div>
</main>

@section scripts{
    <script src="~/Content/js/pdf.js"></script>
    <script src="~/Content/js/bootstrap-pdf-viewer.js"></script>
    <script type="text/javascript">
        ActiveMenu("menu-price-list");
        var viewer;

        $(document).ready(function () {
            viewer = new PDFViewer($('#viewer'));
        });
    </script>
}